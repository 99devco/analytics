{
  "name": "@99devco/analytics",
  "description": "A lightweight JavaScript library for tracking and reporting web browser interactions. Part of the 99dev suite of no-frills web services.",
  "version": "0.0.7",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": ["/dist"],
  "main": "dist/99dev.umd.cjs",
  "module": "dist/99dev.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/99dev.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default":"./dist/99dev.umd.cjs"
      }
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "author": "99dev",
  "license": "MIT",
  "dependencies": { },
  "devDependencies": {
    "typescript": "^5.5.3",
    "vite": "^5.4.8"
  }
}
